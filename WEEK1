function compare(x,y){
  if (typeof x === typeof y ){
    return "SAME TYPE";
    }  
   else ;{
    return "DIFFERENT TYPE";
  }
}

console.log(compare(a,c));
console.log(compare(a,b));
console.log(compare(c,d));
console.log(compare(d,a));
